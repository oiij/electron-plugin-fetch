{
  "name": "electron-plugin-fetch",
  "type": "module",
  "version": "0.0.4",
  "description": "",
  "author": "oiij",
  "license": "MIT",
  "homepage": "https://github.com/oiij/electron-plugin-fetch",
  "repository": {
    "type": "git",
    "url": "git@github.com:oiij/electron-plugin-fetch.git"
  },
  "bugs": "https://github.com/oiij/electron-plugin-fetch/issues",
  "keywords": [
    "electron-plugin-fetch"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./fetch": {
      "types": "./dist/fetch.d.mts",
      "import": "./dist/fetch.mjs"
    },
    "./main": {
      "types": "./dist/main.d.mts",
      "import": "./dist/main.mjs"
    },
    "./preload": {
      "types": "./dist/preload.d.mts",
      "import": "./dist/preload.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsc --noEmit && tsdown",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp -r && npm publish --access public",
    "awe": "pnpx are-we-esm",
    "nmi": "pnpx node-modules-inspector",
    "start": "esno src/index.ts",
    "test": "vitest",
    "update:deps": "taze -w && pnpm i",
    "type:check": "tsc --noEmit",
    "cz": "czg",
    "commit": "git pull && git add -A && pnpm cz && git push",
    "link": "pnpm link --global",
    "preinstall": "npx only-allow pnpm"
  },
  "peerDependencies": {
    "axios": "^1.13.2",
    "electron": "^39.2.7"
  },
  "dependencies": {
    "mime": "^4.1.0",
    "nanoid": "^5.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@oiij/tsconfig": "^0.0.1",
    "@types/node": "^25.0.3",
    "@vitest/ui": "^4.0.16",
    "axios": "^1.13.2",
    "bumpp": "^10.3.2",
    "commitlint": "^20.3.0",
    "cz-git": "^1.12.0",
    "czg": "^1.12.0",
    "electron": "^39.2.7",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.1.0",
    "esno": "^4.8.0",
    "lint-staged": "^16.2.7",
    "simple-git-hooks": "^2.13.1",
    "taze": "^19.9.2",
    "tsdown": "0.19.0-beta.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm type:check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
